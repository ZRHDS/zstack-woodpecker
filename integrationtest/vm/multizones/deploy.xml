            </l3BasicNetwork>
          </l3Networks>
        </l2VlanNetwork>

        <l2VlanNetwork name="$l2VlanNetworkName8" 
          description="guest l2 vlan network 8"
          physicalInterface="$l2NetworkPhysicalInterface" 
          vlan="$l2Vlan8">
          <l3Networks>
            <l3BasicNetwork name="$l3VlanNetwork8" 
              description = "guest test vlan network 8">
